{
  "name": "sample-parse-tree-extension",
  "displayName": "Sample Parse Tree Extension",
  "publisher": "your-publisher",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:sampleParseTree.showNode"
  ],
  "contributes": {
    "commands": [
      {
        "command": "sampleParseTree.showNode",
        "title": "Sample Parse Tree: Show Node",
        "category": "Sample Parse Tree"
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "extensionDependencies": [
    "pokey.parse-tree"
  ],
  "devDependencies": {
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.50.0",
    "@vscode/test-electron": "^2.0.0",
    "typescript": "^4.0.0",
    "web-tree-sitter": "^0.25.3"
  }
}
